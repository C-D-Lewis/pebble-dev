{
  "name": "brackets",
  "author": "Chris Lewis",
  "version": "1.5.0",
  "private": true,
  "keywords": [
    "pebble-app"
  ],
  "pebble": {
    "sdkVersion": "3",
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "enableMultiJS": true,
    "capabilities": [
      "configurable"
    ],
    "projectType": "native",
    "displayName": "Brackets",
    "uuid": "1bd648e8-786a-479c-8609-e2f87f5b706e",
    "watchapp": {
      "watchface": true,
      "hiddenApp": false,
      "onlyShownOnCommunication": false
    },
    "resources": {
      "media": [
        {
          "type": "font",
          "name": "SHARE_20",
          "file": "fonts/Share-TechMono.ttf",
          "characterRegex": "[0-9%]"
        },
        {
          "type": "font",
          "name": "SHARE_24",
          "file": "fonts/Share-TechMono.ttf",
          "characterRegex": "[0-9A-GJ-PR-VY :]"
        },
        {
          "type": "font",
          "name": "SHARE_47",
          "file": "fonts/Share-TechMono.ttf",
          "characterRegex": "[ \\[\\]]"
        },
        {
          "type": "bitmap",
          "name": "MENU_ICON",
          "file": "images/menu_icon.png",
          "menuIcon": true
        }
      ]
    },
    "messageKeys": [
      "BatteryMeter",
      "BluetoothAlert",
      "DashedLine",
      "SecondTick",
      "BackgroundColor",
      "DateColor",
      "TimeColor",
      "BracketColor",
      "LineColor",
      "ComplicationColor"
    ]
  },
  "dependencies": {
    "pebble-clay": "^1.0.4"
  }
}
